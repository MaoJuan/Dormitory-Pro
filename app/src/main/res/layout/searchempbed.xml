<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/topsebRL"
        android:layout_width="wrap_content"
        android:layout_height="55dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="剩余空床数"
            android:textSize="30sp"
            android:textColor="#000000"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/fiveRL"
        android:layout_width="wrap_content"
        android:layout_height="45.0dp"
        android:layout_below="@+id/topsebRL">

        <TextView
            android:id="@+id/fiveTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5号楼："
            android:textColor="#000000"
            android:textSize="30sp" />
        <EditText
            android:id="@+id/fiveET"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:background="#cccccc"
            android:textSize="30sp"
            android:layout_toRightOf="@+id/fiveTV"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/thtRL"
        android:layout_width="wrap_content"
        android:layout_height="45.0dp"
        android:layout_below="@+id/fiveRL">

        <TextView
            android:id="@+id/thtTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13号楼："
            android:textColor="#000000"
            android:textSize="30sp" />
        <EditText
            android:id="@+id/thtET"
            android:layout_width="245dp"
            android:layout_height="35dp"
            android:background="#cccccc"
            android:textSize="30sp"
            android:layout_toRightOf="@+id/thtTV"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/fortRL"
        android:layout_width="wrap_content"
        android:layout_height="45.0dp"
        android:layout_below="@+id/thtRL">

        <TextView
            android:id="@+id/fortTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14号楼："
            android:textColor="#000000"
            android:textSize="30sp" />
        <EditText
            android:id="@+id/fortET"
            android:layout_width="245dp"
            android:layout_height="35dp"
            android:background="#cccccc"
            android:textSize="30sp"
            android:layout_toRightOf="@+id/fortTV"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/eightRL"
        android:layout_width="wrap_content"
        android:layout_height="45.0dp"
        android:layout_below="@+id/fortRL">

        <TextView
            android:id="@+id/eightTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8号楼："
            android:textColor="#000000"
            android:textSize="30sp" />
        <EditText
            android:id="@+id/eightET"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:background="#cccccc"
            android:textSize="30sp"
            android:layout_toRightOf="@+id/eightTV"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/nineRL"
        android:layout_width="wrap_content"
        android:layout_height="45.0dp"
        android:layout_below="@+id/eightRL">

        <TextView
            android:id="@+id/nineTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9号楼："
            android:textColor="#000000"
            android:textSize="30sp" />
        <EditText
            android:id="@+id/nineET"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:background="#cccccc"
            android:textSize="30sp"
            android:layout_toRightOf="@+id/nineTV"/>
    </RelativeLayout>

</RelativeLayout>
